/* Component/main-pingdao/main-pingdao.wxss */

image {
  display: block;
}

.probox {
  margin: 0 30rpx;
  margin-bottom: 30rpx;
  border-bottom: 1rpx solid #dedee1;
}

.probox image {
  width: 100%;
  border-radius: 8rpx;
}

.probox .profoot .p1 {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  color: #427161;
  font-size: 32rpx;
  margin-top: 10rpx;
}

.probox .profoot .p2 {
  color: #9fa0a0;
  font-size: 24rpx;
}

.probox .footbox {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 10rpx 0;
  margin-bottom: 20rpx;
}

.probox .footbox view:nth-child(1) {
  color: #e69888;
  font-size: 40rpx;
}

.probox .footbox view:nth-child(2) {
  color: #fff;
  font-size: 24rpx;
  background: #f5ac9e;
  padding: 6rpx 10rpx;
  border-radius: 30rpx;
}

.probox .zuannum {
  font-size: 26rpx;
  color: #e69888;
  line-height: 24rpx;
}
