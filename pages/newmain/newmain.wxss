/* pages/newmain/newmain.wxss */
page{
  background: #fff;
  overflow:-Scroll;
  overflow-x:hidden;
}
image{
  display: block;
}
::-webkit-scrollbar  
{  
    width: 0;  
    height: 0; 
    color: transparent;  
} 
.topbox{
  position: fixed;
  top:0;
  width: 100%;
  z-index: 999999;
}
.top{
  display: flex;
  align-items: center;
  background: #fff;
    overflow:-Scroll;
  overflow-x: auto;
  width: 100%;
  position: relative;
}
.cloroline{
width: 20%;
position: absolute;
bottom: 0;
left: 0;
transition: left 0.5s
}
.colorxian{
  width: 60%;
  height: 6rpx;
  border-radius: 16px;
  margin: 0 auto;
  background: #427161;
}
.topli{
  width: 20%;
  text-align: center;
  padding: 20rpx 0;
  font-size: 24rpx;
  color: #666666;
  flex-shrink: 0;
}
.slide-image{
  width: 100%;
  height: auto;
}
.heightbox{
  border-radius: 8px;
  overflow: hidden;
}

.navbox{
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  padding: 30rpx 0;
}
.navli{
  width: 20%;
  text-align: center;
  color: #666;
  font-size: 24rpx;
  line-height: 1.6
}
.navimg{
  width: 96rpx;
  height: 96rpx;
  margin: 0 auto;
}

.dianzhu{
  width: 100%;
  border-radius: 8px;
  position: relative;
  color: #fff;
  font-size: 28rpx;
  margin-bottom: 16rpx;
}
.dianzhu image{
  width: 100%;
}
.dianzhu text{
  font-size: 38rpx;
  color: #e69888
}
.dian1{
  position: absolute;
  width: 50%;
  text-align: center;
  left: 0;
  top:0;
  bottom: 0;
  display: flex;
  align-items: center;
  justify-content: center;
}
.dian2{
    position: absolute;
  width: 50%;
  text-align: center;
  right: 0;
  top:0;
  bottom: 0;
    display: flex;
  align-items: center;
  justify-content: center;
}

.advbox {
  display: flex;
  align-items: center;
  margin-top: 14rpx; }
  .advbox .adv1 {
    box-sizing: border-box;
    padding-right: 5rpx;
    width: 50%; }
    .advbox .adv1 image {
      width: 100%; }
  .advbox .adv2 {
    position: relative;
    box-sizing: border-box;
    padding-left: 5rpx;
    width: 50%; }
    .advbox .adv2 image {
      width: 100%; }
    .advbox .adv2 .pro {
      width: 140rpx;
      height: 190rpx;
      position: absolute;
      right: 0;
      bottom: 0; }

.timeout {
  display: flex;
  align-items: center;
  position: absolute;
  left: 9%;
  top: 35%; }

.timetext {
  position: absolute;
  left: 12%;
  top: 60%;
  font-size: 20rpx;
  color: #666464; }

.timenumber {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 44rpx;
  width: 44rpx;
  background: #dfebff;
  border-radius: 50%;
  color: #ca939a;
  font-size: 24rpx; }

.timedian {
  font-size: 28rpx; }

.gaoimg {
  display: flex;
  align-items: center;
  border-radius: 16rpx;
  margin: 26rpx 0; }
  .gaoimg image {
    width: 33.33%; }
  
   .bigtitle{
    color: #1E5543;
    font-size: 35rpx;
    font-weight: 700;
    padding: 30rpx 0;
  }

  .newbox{
    width: 440rpx !important;
    margin-right: 20rpx;
  }

  .protitle{
    	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
  color: #666464;
  font-size: 22rpx;
  height: 64rpx;
  }
  .proprice{
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 10rpx 0;
  }
  .proprice image{
    width: 44rpx;
    height: 44rpx;
    flex-shrink: 0;
  }
  .topmain{
    width: 100%;
    border-radius: 16rpx;
  }
  .projiage{
    color: #E69888;
    font-size: 24rpx;
  }
  .projiage text{
        color: #7C7C7C;
    /* font-size: 18rpx; */
    font-size: 20rpx;
    margin-left: 10rpx;
		text-decoration: line-through;
  }
button::after {
  border: none;
}
.navli button{
  background: #fff;
  text-align:center;
color:#666;
font-size:24rpx;
padding: 0;
line-height: 1.6
}
.louswiper{
  width: 33.3% !important;
}
