.box_container>view{padding: 20rpx 0;border-bottom: 1px solid #F2F2F2}
.box_container>view:nth-last-child(1){margin-bottom: 0;}
.itemsd{display: flex;justify-content: space-between;}
.itemsd text:nth-child(1){
  color: #666464
}
.itemsd text:nth-child(2){
  color: #333
}