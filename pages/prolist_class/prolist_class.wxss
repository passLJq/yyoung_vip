/* pages/prolist_class/prolist_class.wxss */
@import "../../Component/goTop/goTop.wxss";
.NAV_container{width: 100%; height: 88rpx; background-color: #fff;position: fixed; top:0; box-shadow: 0 5px 10px #eee;}
.box_container{display: inline-block; width: 170rpx; height: 100%; line-height: 88rpx; text-align: center; position: relative;}
.select_nav{color: #CDA86E;}
.line_red{position: absolute; top:96%; width:80rpx;height:8rpx;background:#CDA86E;border-radius:4rpx;}
/* 商品列表 */
.list_container{margin-top: 88rpx; padding: 32rpx 24rpx; background-color: #FDFDFD; box-sizing: border-box;flex-wrap: wrap;border-radius:8px;}
.list_container>view{width: 340rpx; height: 478rpx; margin-bottom: 20rpx; background-color: #fff; box-shadow: 0 0 10px #eee;}
.list_img_container{width: 100%; height: 340rpx;}
.list_img_container>image{width: 100%; height: 100%;border-radius:8px;}